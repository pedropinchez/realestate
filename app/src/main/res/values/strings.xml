    <string name="csv_help">
        <![CDATA[
        <p><b>NOTE:</b> All the CSV files should use commas as separators and double quotes (") as quote escapes. Fore more information about
        the CSV file and how to export Excel data to CSV file, please check the following links:</p>
        <p><a href="http://www.computerhope.com/jargon/c/csv.htm">What is CSV file?</a><br/>
        <a href="http://support2.constantcontact.com/articles/FAQ/1868">Export CSV data from Excel</a></p>
        ]]>
    </string>
    <string name="help_import_product_cat">
        <![CDATA[
        <p><b>CSV data line format for product categories:</b></p>
        <p><i>product category name, product category note</i></p>
        <p><b>Examples:</b></p>
        <p>Product category 1,Note 1<br/> 
        Product category 2,Note 2<br/>
        Product category 3,Note 3</p>
        ]]>
    </string>
    <string name="help_import_sale_line">
        <![CDATA[
        <p><b>CSV data line format for sale lines:</b></p>
        <p><i>line name,line address</i></p>
        <p><b>Examples:</b></p>
        <p>Line 1,Ha noi<br/>
		Line 2,Bac Giang<br/>
		Line 3,Thai Binh</p>
        ]]>
    </string>
    <string name="help_import_product">
        <![CDATA[
        <p><b>CSV data line format for products:</b></p>
        <p><i>product name,product code, unit price, BOGO purchase quantity, BOGO get quantity</i></p>
        <p><b>Examples:</b></p>
        <p>Product 1,PROD-1,100,10,1<br/>
        Product 2,PROD-2,150,0,0<br/>
        Product 3,PROD-3,200,,</p>
        ]]>
    </string>
    <string name="help_import_customer">
        <![CDATA[
        <p><b>CSV data line format for customers:</b></p>
        <p><i>shop name,owner name,address,mobile phone,landline phone,note,evaluation,rate</i></p>
        <p><b>Examples:</b></p>
        <p>Shop 1,Owner 1,Ha Noi,099959552,044233543,No note,Every potential,3<br/>
        Shop 2,Owner 2,Ha Noi,099900552,044211543,No note,Every potential,5<br/>
        Shop 3,Owner 3,Ha Noi,099959552,,,Every potential,3</p>
        ]]>
    </string>
    
